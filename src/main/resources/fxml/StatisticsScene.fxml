<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.unideb.inf.poker.Controller.StatisticsSceneController">
   <children>
      <BarChart fx:id="cardStrengthBarChart" layoutX="286.0" layoutY="119.0" title="YOUR HANDS WERE:">
        <xAxis>
          <CategoryAxis label="HANDS TYPE" side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" label="NUMBER" side="LEFT" upperBound="10.0" />
        </yAxis>
      </BarChart>
      <Label layoutX="45.0" layoutY="119.0" text="YOUR WINRATE: ">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label fx:id="winRateLabel" layoutX="115.0" layoutY="157.0" text="Label">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label layoutX="15.0" layoutY="262.0" text="AVARAGE PRIZE WON:">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label fx:id="prizeWinLabel" layoutX="114.0" layoutY="300.0" text="Label">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label layoutX="15.0" layoutY="424.0" text="AVARAGE PRIZE LOST:">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label fx:id="prizeLostLabel" layoutX="115.0" layoutY="462.0" text="Label">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="mainMenuButton" layoutX="159.0" layoutY="36.0" mnemonicParsing="false" onAction="#mainMenuButtonAction" text="Main Menu" />
      <Button fx:id="quitButton" layoutX="536.0" layoutY="36.0" mnemonicParsing="false" onAction="#quitButtonAction" text="Quit" />
   </children>
</AnchorPane>
